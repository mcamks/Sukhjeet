
@{
    ViewBag.Title = "Research";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .trading_view{padding-top:6px !important;}
</style>
<div style="width:100%;">
    @Html.Partial("_YahooFinanceApp")

    <input id="codeIntoSession" type="hidden" value="@ViewBag.Code" />
    <script>
        $(document).ready(function () {
            if ($('#codeIntoSession').val() == "") {
                OpentheMarkeResourceDataFromDashboard('SPY');
            } else { OpentheMarkeResourceDataFromDashboard($('#codeIntoSession').val()); }
        });

    </script>
</div>