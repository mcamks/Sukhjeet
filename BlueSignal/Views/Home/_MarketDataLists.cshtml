
<div class="tab_block">
    <div class="">
        <div class="col-sm-12 ">
            <div class="info_block" style="margin-bottom:10px;">
                <div class="blue_block">
                    <div class="col-sm-6">
                        <div class="row">
                            <label class="col-sm-3 symbol_text" for="code">Symbol:</label>
                            <div class="col-sm-6 search_symbol">
                                <input type="text" id="code" name="code" placeholder="Type valid Symbol" data-toggle="tooltip" title="Type a Valid Symbol" value="SPY" class="form-control codeValue">
                                <span onclick="InitilizeMarketList()"><i class="fa fa-search" data-toggle="tooltip" title="Lookup Symbol" aria-hidden="true"></i></span>
                            </div>
                            <div class="col-sm-3">
                                @*<input type="button" class="btn btn-primary" data-toggle="modal" data-target="#myModal" value="Choose Major Market">*@
                                @*<i class="fa fa-window-maximize" title="Major Market" aria-hidden="true" data-toggle="modal" data-target="#myModal"></i>*@
                                <button type="button" id="btnLoad" data-toggle="tooltip" title="Load Symbol" class="btn btn-primary btn_style btn_whitegray" onclick="InitilizeMarketList()">Load</button>
                            </div>
                        </div>
                    </div>
                    
                </div>
                
            </div>
        </div>
        <div class="col-sm-12">
            <div class="panel-group accordion">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title"> <a data-toggle="collapse" href="#collapseDaily">Market Data - Daily</a> </h4>
                    </div>
                    <div id="collapseDaily" class="panel-collapse collapse in">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div style="overflow:auto;">
                                        <div class="table-responsive table_style">
                                            <table id="Id_MarketDataListDaily" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                                                <thead>
                                                    <tr>
                                                        <th>Symbol</th>
                                                        @*<th>TimeStamp</th>*@
                                                        <th>Trading Date</th>
                                                        <th>Open</th>

                                                        <th>High</th>
                                                        <th>Low</th>
                                                        <th>Close</th>
                                                        <th>Volume</th>
                                                        <th>Open Interest</th>
                                                    </tr>
                                                </thead>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>